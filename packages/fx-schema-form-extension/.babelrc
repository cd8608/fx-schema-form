{
    "env": {
        "production": {
            "presets": [
                ["env", {
                    "targets": {},
                    "modules": false,
                    "loose": true
                }]
            ],
            "plugins": [
                ["transform-runtime", {
                    "helpers": false,
                    "modules": false,
                    "polyfill": false,
                    "regenerator": true,
                    "moduleName": "babel-runtime"
                }]
            ],
            "minified": false
        },
        "development": {
            "presets": [
                "env", "react"
            ],
            "plugins": [
                ["transform-runtime", {
                    "helpers": true,
                    "modules": false,
                    "polyfill": false,
                    "regenerator": true,
                    "moduleName": "babel-runtime"
                }]
            ]
        }
    },
    "comments": true
}